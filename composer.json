{
	"name": "fromholdio/silverstripe-attributable",
	"type": "silverstripe-vendormodule",
	"description": "A SilverStripe module providing a global attribution system for categorising, tagging, etc any/all objects and/or pages across your project.",
	"license": "BSD-3-Clause",
	"keywords": ["silverstripe", "attribution", "attribute", "category", "tag", "taxonomy", "related"],
	"homepage": "https://github.com/fromholdio/silverstripe-attributable",
	"support": {
		"issues": "https://github.com/fromholdio/silverstripe-attributable/issues"
	},
	"authors": [{
		"name": "Luke Fromhold",
		"homepage": "https://fromhold.io"
	}],
    "require": {
        "php": ">=5.6",
        "silverstripe/vendor-plugin": "^1.0",
        "silverstripe/framework": "^4.0",
        "fromholdio/silverstripe-commonancestor": "^1.0"
    },
	"autoload": {
		"psr-4": {
			"Fromholdio\\Attributable\\": "src/"
		}
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
